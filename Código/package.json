{
  "name": "projeto-de-extensao-ii",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "fdgfg",
  "license": "ISC",
  "description": "dfgdfgdg",
  "build": {
    "appId": "com.exemplo.luz_da_lua",
    "productName": "LuzDaLua",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "index.js",
      "db/",
      "helpers/",
      "views/",
      "controllers/",
      "models/",
      "public/",
      "routes/",
      "node_modules/",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "luz_da_lua_db.sqlite",
        "to": "app/luz_da_lua_db.sqlite"
      },
      {
        "from": "public/img/logo.ico",
        "to": "app/logo.ico"
      }
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "icon": "public/img/logo.ico",
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "mac": {
      "target": "dmg"
    }
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.7",
    "cookie-session": "^2.1.0",
    "express": "^5.1.0",
    "express-flash": "^0.0.2",
    "express-handlebars": "^8.0.1",
    "express-session": "^1.18.1",
    "handlebars": "^4.7.8",
    "nodemon": "^3.1.9",
    "puppeteer": "^24.11.2",
    "sequelize": "^6.37.7",
    "session-file-store": "^1.5.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "prettier": "3.6.2"
  }
}
